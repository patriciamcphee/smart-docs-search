{
  "name": "smart-search-plugin",
  "version": "3.0.0",
  "description": "A modern search plugin for Docusaurus 3.x that provides enhanced search functionality with a clean, user-friendly interface.",
  "author": "Patricia McPhee <patti@pixlngrid.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/patriciamcphee/smart-docs-search.git"
  },
  "bugs": {
    "url": "https://github.com/patriciamcphee/smart-docs-search/issues"
  },
  "homepage": "https://github.com/patriciamcphee/smart-docs-search#readme",
  "keywords": [
    "docusaurus",
    "docusaurus-plugin",
    "search",
    "documentation",
    "react",
    "local search",
    "docusaurus-3"
  ],
  "main": "lib/index.js",
  "files": [
    "lib",
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf lib/",
    "build": "npm run clean && cross-env NODE_ENV=production babel src -d lib --copy-files",
    "watch": "babel src -d lib --copy-files --watch",
    "prepare": "npm run build",
    "test": "jest",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "antd": "^5.12.8",
    "fs-extra": "^11.2.0",
    "fuse.js": "^7.0.0",
    "gray-matter": "^4.0.3",
    "lodash": "^4.17.21",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "unified": "^11.0.4",
    "unist-util-visit": "^5.0.0"
  },
  "peerDependencies": {
    "@docusaurus/core": "^3.0.0",
    "@docusaurus/theme-classic": "^3.0.0",
    "@docusaurus/theme-common": "^3.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@docusaurus/module-type-aliases": "^3.0.0",
    "@docusaurus/types": "^3.0.0",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
